<?xml version='1.0' encoding='UTF-8'?>
<!--DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
		"http://www.springframework.org/dtd/spring-beans.dtd"-->
<beans
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:qvp="http://www.qvp.com/schema/qvp"
	    xsi:schemaLocation="http://www.springframework.org/schema/beans
	    http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
	   		http://www.qvp.com/schema/qvp http://www.qvp.com/schema/qvp/spring-qvp-2.0.xsd"
		>


	<bean id="qvpContext" class="com.bmn.spring.app.QvpContext" lazy-init="true">
		<property name="id" value="3333"/>
		<property name="name" value="qvp"/>
	</bean>


	<qvp:annotation kind="direct">
		<qvp:expected-type type="expected-type"/>
		<qvp:assignable-to type="assignable-to"/>
		<qvp:exports type="exports" identifier="10"/>
	</qvp:annotation>
</beans>
